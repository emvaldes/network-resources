{
  "target": "172.16.1.6",
  "configs": [
    {
      "site": "cisco",
      "device": "fw2.cfg",
      "objects": [
        {
          "object": "object-group",
          "type": "network Batcave-Wireless",
          "description": "Secure wireless endpoints",
          "entries": [
            "network-object host 172.16.1.6",
            "network-object host 192.168.100.100"
          ],
          "group": "Unclassified"
        }
      ]
    },
    {
      "site": "cisco",
      "device": "fw5.cfg",
      "objects": [
        {
          "object": "access-list",
          "type": false,
          "description": false,
          "entries": [
            "access-list OUTSIDE extended permit ip host 172.16.1.6 any"
          ],
          "group": "Unclassified"
        },
        {
          "object": "object",
          "type": "network BlackWidow-Core",
          "description": false,
          "entries": [
            "host 172.16.1.6"
          ],
          "group": "Unclassified"
        },
        {
          "object": "object-group",
          "type": "network RedRoom-Core",
          "description": "Classified subnets",
          "entries": [
            "network-object host 10.0.0.1",
            "network-object host 172.16.1.6"
          ],
          "group": "Unclassified"
        }
      ]
    },
    {
      "site": "misc",
      "device": "switch3.conf",
      "objects": [
        {
          "object": "interface",
          "type": "Vlan2",
          "description": false,
          "entries": [
            "ip address 172.16.1.6 255.255.255.0"
          ],
          "group": "Business-Unit-Networks"
        }
      ]
    }
  ]
}
